<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd" >

    <bean id="jmx.jmxPostStartupService" class="fr.paris.lutece.plugins.jmx.service.JMXPostStartupService" />
 
    <bean id="jmx.portalMBeanManager" class="fr.paris.lutece.plugins.jmx.mbeans.portal.PortalMBeanManager" />
    <bean id="jmx.pluginMBeansManager" class="fr.paris.lutece.plugins.jmx.mbeans.plugins.PluginMBeansManager" />
    <bean id="jmx.cacheMBeansManager" class="fr.paris.lutece.plugins.jmx.mbeans.caches.CacheMBeansManager" />
       
    <bean id="jmx.jmxService" class="fr.paris.lutece.plugins.jmx.service.JMXService" >
        <property name="beanManagersList">
            <list>
                <ref bean="jmx.portalMBeanManager" />
            </list>
        </property>
        <property name="resourceManagersList">
            <list>
                <ref bean="jmx.pluginMBeansManager" />
                <ref bean="jmx.cacheMBeansManager" />
            </list>
        </property>
    </bean>
</beans>
